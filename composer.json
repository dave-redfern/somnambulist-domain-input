{
    "name": "somnambulist/domain-input",
    "type": "library",
    "description": "A collection of Domain Input/Response classes to act as a bridge between domain and environment.",
    "keywords": [
        "domain",
        "ddd",
        "input mapping"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "info@somnambulist.tech"
        }
    ],
    "require": {
        "php": ">=7.4",
        "somnambulist/collection": "~3.0|~4.0",
        "symfony/http-foundation": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\Domain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Tests\\Domain\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
